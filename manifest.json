{
  "name": "Divimode Support",
  "description": "Support tools for Divimode plugins",
  "version": "1.0",
  "manifest_version": 3,
  "update_url": "https://divimode.com/crx/divimode-support.xml",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_title": "Inspect contents of the current page",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "injected.js",
        "global-search.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
